extends layout

block content
	.container
		form(method="get" action="show")
			label(for="date") Date:
			input(type="date", name="date", min="2014-11-01", max="2015-12-31", value= logDate)
	
			label(for="mtCode") MT Code:
			input(type="text", name="mtCode")
	
			label(for="isErrorOnly") ErrorOnly
			input(type="checkbox", name="isErrorOnly")
	
			input(type="submit", value="Search")
	
		include monitorLogTable
